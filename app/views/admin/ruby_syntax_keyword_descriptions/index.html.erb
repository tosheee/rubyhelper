<div class="form-index">
<p id="notice"><%= notice %></p>

  <div class="heading">
    <h4>Listing Admin Ruby Syntax Keyword Descriptions</h4>
    | <%= link_to 'Elements', admin_ruby_syntax_name_elements_path %>
    | <%= link_to 'Keywords', admin_ruby_syntax_name_keywords_path %>
    | <%= link_to 'New Ruby syntax keyword description', new_admin_ruby_syntax_keyword_description_path %> |
  </div>


  <table>
  <thead>
    <tr>
      <th>Ruby syntax name keyword</th>
      <th>Num view</th>
      <th>Label description</th>
      <th>Full keyword description</th>
      <th>Label code</th>
      <th>Keyword code</th>
      <th>Label result</th>
      <th>Result code</th>
      <th>Show</th>
      <th>Edit</th>
      <th>Destroy</th>
    </tr>
  </thead>

  <tbody>
    <% @admin_ruby_syntax_keyword_descriptions.each do |admin_ruby_syntax_keyword_description| %>
      <tr>
        <td><%= Admin::RubySyntaxNameKeyword.find(admin_ruby_syntax_keyword_description.ruby_syntax_name_keyword_id).name_keyword %></td>
        <td><%= admin_ruby_syntax_keyword_description.num_view %></td>
        <td><%= admin_ruby_syntax_keyword_description.label_description %></td>
        <td><%= admin_ruby_syntax_keyword_description.full_keyword_description %></td>
        <td><%= admin_ruby_syntax_keyword_description.label_code %></td>
        <td><%= admin_ruby_syntax_keyword_description.keyword_code %></td>
        <td><%= admin_ruby_syntax_keyword_description.label_result %></td>
        <td><%= admin_ruby_syntax_keyword_description.result_code %></td>
        <td><%= link_to 'Show', admin_ruby_syntax_keyword_description %></td>
        <td><%= link_to 'Edit', edit_admin_ruby_syntax_keyword_description_path(admin_ruby_syntax_keyword_description) %></td>
        <td><%= link_to 'Destroy', admin_ruby_syntax_keyword_description, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>
